import aaz.libhpsocket.listener.server;
import aaz.libhpsocket.component.tcpPackServer;

var _dll = aaz.libhpsocket._dll;

namespace aaz.libhpsocket.listener;
class tcpPackServer{
	ctor(){
		this = ..aaz.libhpsocket.listener.server("component.tcpPackServer");
		this.pListener = _dll.Create_HP_TcpPackServerListenerP();
	}

	destroy = function(){
		if(this.pListener){
			_dll.Destroy_HP_TcpPackServerListener(this.pListener);
			this.pListener = null;
		}
	}
}