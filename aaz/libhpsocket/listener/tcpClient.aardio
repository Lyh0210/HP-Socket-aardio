import aaz.libhpsocket.listener.client;
import aaz.libhpsocket.component.tcpClient;

var _dll = aaz.libhpsocket._dll;

namespace aaz.libhpsocket.listener;
class tcpClient{
	ctor(){
		this = ..aaz.libhpsocket.listener.client("component.tcpClient");
		this.pListener = _dll.Create_HP_TcpClientListenerP();
	}

	destroy = function(){
		if(this.pListener){
			_dll.Destroy_HP_TcpClientListener(this.pListener);
			this.pListener = null;
		}
	}
}